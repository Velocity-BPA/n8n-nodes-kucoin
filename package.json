{
  "name": "n8n-nodes-kucoin",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for KuCoin exchange providing spot, futures, margin, HF trading, lending, earn products, and sub-account management with 90+ operations.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-kucoin.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-kucoin#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-kucoin/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "kucoin",
    "cryptocurrency",
    "exchange",
    "trading",
    "spot",
    "futures",
    "margin",
    "hf-trading",
    "high-frequency",
    "lending",
    "earn",
    "api",
    "automation",
    "workflow"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/KuCoinApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/KuCoin/KuCoin.node.js",
      "dist/nodes/KuCoin/KuCoinTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
